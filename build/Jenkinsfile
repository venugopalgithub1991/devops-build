pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
               checkout scmGit(branches: [[name: '**']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/venugopalgithub1991/devops-build']])
            }
        }
            stage('push to dev'){
                steps{
                    sh'sudo docker tag devops-buid docker-repo'
                    sh'sudo docker push docker-repo-url'
                }
                    
                }
                stage('push to main'){
                steps{
                    sh'sudo docker tag devops-buid docker-repo'
                    sh'sudo docker push docker-repo-url'
                }
                    
                }
                
                
            }
        
    }
